{
  "name": "paintquote-pro-mcp",
  "version": "1.0.0",
  "description": "MCP configuration for PaintQuote Pro application",
  "servers": {
    "serena": {
      "description": "Serena MCP server for enhanced AI capabilities",
      "enabled": true,
      "config": {
        "command": "npx",
        "args": ["-y", "serena-mcp"],
        "env": {
          "SERENA_API_KEY": "${SERENA_API_KEY}",
          "PROJECT_PATH": "/Users/sepg/Desktop/projects/painttest4"
        }
      }
    }
  },
  "features": {
    "codeAnalysis": true,
    "projectManagement": true,
    "aiAssistance": true,
    "documentationGeneration": true
  },
  "project": {
    "name": "PaintQuote Pro",
    "type": "Next.js Application",
    "description": "AI-powered painting quote generation platform",
    "primaryLanguages": ["TypeScript", "JavaScript"],
    "frameworks": [
      "Next.js 14",
      "React 18",
      "Tailwind CSS",
      "Prisma ORM"
    ],
    "database": "PostgreSQL",
    "aiIntegrations": [
      "Anthropic Claude",
      "OpenRouter"
    ]
  }
}