# Database Configuration
NEXT_PUBLIC_SUPABASE_URL=your-supabase-url-here
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key-here
SUPABASE_SERVICE_ROLE_KEY=your-supabase-service-role-key-here

# AI Configuration
OPENROUTER_API_KEY=your-openrouter-api-key-here
# You can get an API key from https://openrouter.ai/keys

# Stripe Configuration (Optional for local testing)
STRIPE_SECRET_KEY=your-stripe-secret-key-here
STRIPE_WEBHOOK_SECRET=your-stripe-webhook-secret-here
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=your-stripe-publishable-key-here

# Email Configuration (Optional)
RESEND_API_KEY=your-resend-api-key-here

# App Configuration
NEXT_PUBLIC_APP_URL=http://localhost:3005
NODE_ENV=development

# N8N Integration Configuration
N8N_WEBHOOK_BASE_URL=https://your-n8n-instance.com
N8N_API_KEY=your-n8n-api-key-here
# Get your N8N webhook URL from your N8N instance
# Example: https://n8n.yourdomain.com or https://yourworkspace.n8n.cloud

# Copy this file to .env.local and fill in your actual values
# The values in Vercel will be used for production