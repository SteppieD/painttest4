{
  "name": "paintquotepro-unified",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev --port 3005",
    "dev:turbo": "next dev --port 3005 --turbo",
    "build": "next build",
    "start": "next start --port 3005",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,md,json}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,md,json}\"",
    "typecheck": "tsc --noEmit",
    "check:all": "npm-run-all --parallel typecheck lint:fix format",
    "setup-db": "node -e \"console.log('Database auto-initialized on first run')\"",
    "db:studio": "npx prisma studio",
    "db:push": "npx prisma db push",
    "db:migrate": "npx prisma migrate dev",
    "db:seed": "npx prisma db seed",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:debug": "playwright test --debug",
    "test:links": "node tests/link-audit.js",
    "test:accessibility": "node tests/accessibility-audit.js",
    "test:payments": "node tests/payment-flow-test.js",
    "test:tiers": "node tests/dashboard-tier-test.js",
    "test:all": "node tests/run-all-tests.js",
    "deps:check": "npm-check-updates",
    "deps:update": "npm-check-updates -u",
    "deps:audit": "npm audit",
    "clean": "rm -rf .next node_modules .turbo",
    "clean:install": "npm run clean && npm install",
    "analyze": "ANALYZE=true npm run build",
    "cli": "node scripts/cli.js",
    "docker:build": "docker-compose -f docker-compose.vercel.yml build",
    "docker:up": "docker-compose -f docker-compose.vercel.yml up",
    "docker:down": "docker-compose -f docker-compose.vercel.yml down",
    "docker:test": "docker-compose -f docker-compose.vercel.yml up -d && echo 'Testing at http://localhost:3000'",
    "deploy": "./scripts/deploy.sh",
    "prepare": "husky"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.54.0",
    "@google/generative-ai": "^0.21.0",
    "@hookform/resolvers": "^3.9.0",
    "@radix-ui/react-accordion": "^1.2.11",
    "@radix-ui/react-avatar": "^1.1.0",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.1",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slider": "^1.3.5",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toast": "^1.2.1",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@stripe/react-stripe-js": "^3.7.0",
    "@stripe/stripe-js": "^7.4.0",
    "@supabase/supabase-js": "^2.39.1",
    "axios": "^1.11.0",
    "bcryptjs": "^3.0.2",
    "better-sqlite3": "^11.6.0",
    "canvas-confetti": "^1.9.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "decimal.js": "^10.6.0",
    "framer-motion": "^11.18.2",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.436.0",
    "next": "^14.2.30",
    "next-auth": "^4.24.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.52.1",
    "react-icons": "^5.2.1",
    "recharts": "^3.1.0",
    "resend": "^4.7.0",
    "stripe": "^18.3.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "web-vitals": "^5.0.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@playwright/test": "^1.54.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/better-sqlite3": "^7.6.13",
    "@types/decimal.js": "^0.0.32",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "20.19.1",
    "@types/react": "18.3.23",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "autoprefixer": "^10.4.16",
    "dotenv": "^17.2.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.5",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "node-fetch": "^2.7.0",
    "npm-run-all2": "^8.0.4",
    "playwright": "^1.54.2",
    "postcss": "^8.4.32",
    "prettier": "^3.6.2",
    "puppeteer": "^24.15.0",
    "tailwindcss": "^3.3.6",
    "typescript": "5.8.3"
  }
}
